<!DOCTYPE html>
<html>
<head>
  <title>BCYDatabase</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navbar">
    <div class="login-section">
      <% if current_user %>
        <%= link_to "Logout", session_path(current_user), method: :delete, class: :destroy %>
      <% end %>
    </div>
  </div>
<%= yield %>

</body>
</html>
